{
    "name": "andreas-weber/php-junit-merge",
    "type": "library",
    "description": "PHP-Unit J-Unit XML File Merger",
    "homepage": "https://github.com/andreas-weber/php-junit-merge",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Weber",
            "email": "code@andreas-weber.me",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/andreas-weber/php-junit-merge/issues"
    },
    "require": {
        "php": ">=5.6",
        "symfony/console": "^3.0 || ^4.0",
        "symfony/finder": "^3.0 || ^4.0 || ^5.0",
        "theseer/fdomdocument": "1.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "phploc/phploc": "4.*",
        "squizlabs/php_codesniffer": "1.5.*",
        "phpmd/phpmd": "2.*",
        "sebastian/phpcpd": "2.0.*"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "config": {
        "bin-dir": "vendor/bin/"
    },
    "bin": [
        "phpjunitmerge"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
